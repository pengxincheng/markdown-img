// 监控FPS
;(function(){
  var clock = window.performance || Date;
  var lastTime = clock.now();
  var frame = 0;
  var lastFameTime = clock.now();
  var fpsCollection = [];
  var disable = false;
  var max = 60 * 5;
  var loop = function () {
    if (disable) return;
    var now = clock.now();
    var fs = now - lastFameTime;
    lastFameTime = now;
    var fps = Math.round(1000 / fs);
    frame += 1;
    if (now > 1000 + lastTime) {
      fps = Math.round((frame * 1000) / (now - lastTime));
      frame = 0;
      lastTime = now;
      if (fpsCollection.length > max) {
        disable = true;
      }
      fpsCollection.push(fps);
    }
    window.requestAnimationFrame && window.requestAnimationFrame(loop);
  };
  window.fps = {
    get: function(){
      return [].concat(fpsCollection);
    },
    stop: function() {
      disable = true;
    },
    restart: function() {
      disable = false;
      fpsCollection = [];
      var lastTime = clock.now();
      var frame = 0;
      var lastFameTime = clock.now();
      loop();
    }
  }
  loop();
})();
